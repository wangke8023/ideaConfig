<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="springboot.dao.queryUserDAO">
	<select id="queryUserById" parameterType="java.lang.String" resultType="java.lang.String">
		select 1 from dual
	</select>
	<select id="queryUserObj" parameterType="java.lang.String" resultType="springboot.dto.User">
		select 1 as id from dual
	</select>
	
</mapper>


